!xrdb -merge ~/.Xresource
!xrdb ~/.Xresource
!--- urxvt
urxvt.font: xft:Source Code Pro:style=Regular:size=10,xft:Migmix 1P:size=10,xft:Noto Sans CJK JP:size=10,xft:DejaVu Sans Mono:size=10

urxvt.perl-ext-common:default,clipboard,matcher,keyboard-select,font-size
urxvt.keysym.C-Up:    perl:font-size:increase
urxvt.keysym.C-Down:  perl:font-size:decrease
urxvt.keysym.M-u:     perl:url-select:select_next
urxvt.url-launcher:   /usr/bin/xdg-open
urxvt.underlineURLs:  True
urxvt.matcher.button: 1 
urxvt.keysym.M-Escape:perl:keyboard-select:activate
urxvt.keysym.M-c:     perl:clipboard:copy
urxvt.keysym.M-v:     perl:clipboard:paste
urxvt.keysym.M-C-v:   perl:clipboard:paste_escaped
urxvt.copyCommand:   xsel -ib
urxvt.pasteCommand:  xsel -ob
!URxvt.clipboard.autocopy: true
 

urxvt.depth: 32
!URxvt.transparent: true
!URxvt.shading: 15
urxvt.background: [85]#000000
urxvt.foreground: #E4ECDC

! black
*color0:  #687478
*color8:  #A5A7A3
! red
*color1:  #a40000
*color9:  #EF2929
! green
*color2:  #4E9A06
*color10: #8AE234
! yellow
*color3:  #C4A000
*color11: #FCE94F
! blue
*color4:  #3465A4
*color12: #729FCF
! purple
*color5:  #75507B
*color13: #AD7FA8
! orange (replaces cyan)
! *color6:  #ce5c00
